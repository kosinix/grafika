{
    "name": "kosinix/grafika",
    "description": "An image manipulation library for PHP.",
    "keywords": ["grafika"],
    "homepage": "http://kosinix.github.io/grafika",
    "type": "library",
    "license": [
        "MIT",
        "GPL-2.0+"
    ],
    "authors": [
        {
            "name": "Nico Amarilla",
            "homepage": "https://www.kosinix.com"
        }
    ],
    "require": {
        "php": ">=7.3"
    },
    "suggest": {
        "ext-gd": "*",
        "ext-imagick": "*"
    },
    "autoload": {
        "psr-4": {
            "Grafika\\": "src/Grafika"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/var-dumper": "^5.0"
    }
}
